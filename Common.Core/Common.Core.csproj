<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>MitherPack.Common.Core</PackageId>
		<Authors>Renato Mitherhofer</Authors>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<NoWarn>$(NoWarn);1591</NoWarn>
		<Version>9.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MitherPack.Common.Exceptions" Version="9.0.0" />
		<PackageReference Include="MitherPack.Common.Notifications" Version="9.0.0" />
	</ItemGroup>

</Project>
