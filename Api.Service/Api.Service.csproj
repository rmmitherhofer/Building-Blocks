<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>MitherPack.Api.Service.Framework</PackageId>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Authors>Renato Mitherhofer</Authors>
		<NoWarn>$(NoWarn);1591</NoWarn>
		<Version>1.0.1</Version>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="AutoMapper" Version="13.0.1" />
		<PackageReference Include="FluentValidation" Version="11.10.0" />
		<PackageReference Include="MitherPack.Api.Requests" Version="1.0.1" />
		<PackageReference Include="MitherPack.Api.Swagger" Version="1.0.1" />
		<PackageReference Include="SnapTrace" Version="1.0.1" />
	</ItemGroup>

</Project>
