    <PackageReference Include="MitherPack.Api.Requests" Version="1.0.0" />
    <PackageReference Include="MitherPack.Api.Responses" Version="1.0.0" />
    <PackageReference Include="MitherPack.Api.Swagger" Version="1.0.0" />
    <PackageReference Include="MitherPack.Common.Exceptions" Version="1.0.0" />
    <PackageReference Include="MitherPack.Common.Extensions" Version="1.0.0" />
    <PackageReference Include="MitherPack.Common.Notifications" Version="1.0.0" />
  </ItemGroup>

</Project>
