<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<NoWarn>$(NoWarn);1591</NoWarn>

		<Version>9.0.4</Version>

		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>

		<Authors>Renato Mitherhofer</Authors>

		<PackageId>MitherPack.Common.Http</PackageId>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="MitherPack.Api.Responses" Version="9.0.3" />
	  <PackageReference Include="MitherPack.Common.Json" Version="9.0.4" />
	  <PackageReference Include="MitherPack.Common.User" Version="9.0.3" />
	</ItemGroup>

</Project>
